package day1;

public class test {
public static void main(String[] args) {
Student student1=new Student(1, "张1", "男", 15, 94);
Student student2=new Student(2, "张2", "男", 15, 90);
Student student3=new Student(3, "张3", "男", 15, 96);
Student student4=new Student(4, "张4", "男", 15, 99);
Student student5=new Student(5, "张5", "男", 15, 95);

}
}
class Student{
	private int sNo;
	private String sName;
	private String sSex;
	private int sAge;
	private int  sJava;
	public Student(int sNo, String sName, String sSex, int sAge,int sJava) {
		super();
		this.sNo = sNo;
		this.sName = sName;
		this.sSex = sSex;
		this.sAge = sAge;
		this.sJava = sJava;
	}
	public int getNo() {
		return sNo;
	}
	public void setNo(int sNo) {
		this.sNo = sNo;
	}
	public String getName() {
		return sName;
	}
	public void setName(String sName) {
		this.sName = sName;
	}
	public String getSex() {
		return sSex;
	}
	public void setSex(String sSex) {
		this.sSex = sSex;
	}
	public int getAge() {
		return sAge;
	}
	public void setAge(int sAge) {
		this.sAge = sAge;
	}
	public int getJava() {
		return sJava;
	}
	public void setJava(int sJava) {
		this.sJava = sJava;
	}
	public void show() {
		System.out.println(sNo+" "+sName+" "+sSex+" "+sAge+" "+sJava);
	}
}
